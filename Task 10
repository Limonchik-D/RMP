Готовый преокт: https://snack.expo.dev/@david7_7/task-10

если не работает:
сздать проект в снеке, добавить файл с именем Dog.tsx
в него вставить код: import React from 'react';
import { StyleSheet, Text, View } from 'react-native';

// Определяем тип пропсов для компонента Dog
type DogProps = {
  name: string;
};

const Dog: React.FC<DogProps> = ({ name }) => {
  return (
    <View style={styles.rectangle}>
      <Text style={styles.text}>{name}</Text>
    </View>
  );
};

const styles = StyleSheet.create({
  rectangle: {
    width: '100%', // Растягиваем по ширине
    height: 100, // Высота 100 пикселей
    backgroundColor: 'green',
    alignItems: 'center',
    justifyContent: 'center',
  },
  text: {
    color: 'white', // Белый текст для контраста
    fontSize: 20,
  },
});

export default Dog;

*********************

в App.tsx вставь код:
import React from 'react';
import { StyleSheet, View } from 'react-native';
import Dog from './Dog';

export default function App() {
  return (
    <View style={styles.container}>
      <Dog name="Шарик1" />
      <Dog name="Шарик2" />
      <Dog name="Шарик3" />
    </View>
  );
}

const styles = StyleSheet.create({
  container: {
    flex: 1,
    justifyContent: 'center',
    alignItems: 'center',
    backgroundColor: '#fff',
    gap: 20, // Расстояние между прямоугольниками
  },
});
