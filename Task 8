если не работает то вот код для снека:
import React from 'react';
import { StyleSheet, View } from 'react-native';

export default function App() {
  return (
    <View style={styles.container}>
      <View style={[styles.circle, { backgroundColor: 'red' }]} />
      <View style={[styles.circle, { backgroundColor: 'yellow' }]} />
      <View style={[styles.circle, { backgroundColor: 'green' }]} />
    </View>
  );
}

const styles = StyleSheet.create({
  container: {
    flex: 1,
    alignItems: 'center',
    justifyContent: 'center',
    backgroundColor: '#fff',
    gap: 20, // Расстояние между окружностями
  },
  circle: {
    width: 100, // Диаметр = 2 * радиус (50 * 2)
    height: 100,
    borderRadius: 50, // Радиус = половина ширины/высоты для создания окружности
  },
});
